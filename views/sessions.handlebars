<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h4>Пользователь: <span class="member-name"></span> <br /> Добро пожаловать!</h4>
    </div>
  </div>
</div>
<div class="container">
  <div class="section">
    <div class="row">
      <div class="col s12 m2">
      </div>
      <div class="col s12 m8">
        <h5>Список сеансов пользователей</h5>
        </br>
        </br>
        <ul class="collection">
          {{#each sessions}}
          <li class="collection-item avatar">
            <i class="material-icons circle {{this.[user.color]}}"></i>
            <p>{{this.[user.firstName]}} {{this.[user.lastName]}} </p>
            <p class="secondary-content">
              Время сеанса: {{this.date}}<br>
            </p>
          </li>
          {{/each}}
        </ul>
        </br>
        </br>
      </div>
      <div class="col s12 m2">
      </div>
    </div>
  </div>
</div>