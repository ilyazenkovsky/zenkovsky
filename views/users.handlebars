<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h4>Пользователь: <span class="member-name"></span> <br /> Добро пожаловать!</h4>
    </div>
  </div>
</div>
<div class="container">
  <div class="section">
    <div class="row">
      <div class="col s12 m2">
      </div>
      <div class="col s12 m8">
        <h5>Список пользователей</h5>
        </br>
        </br>
        <ul class="collection">
          {{#each users}}
          <li class="collection-item avatar">
            <i class="material-icons circle {{this.color}}"></i>
            <p>{{this.firstName}} {{this.lastName}}<br>
              <img src="{{this.badgeUrl}}" alt="Completed Tasks: >{{this.TicketsAssignedclosed}}"
                title="Completed Tasks: >{{this.TicketsAssignedclosed}}" />
            </p>
            <p class="secondary-content">
              Открытые задачи: {{this.TicketsAssignedOpen}}<br>
              Созданные задачи: {{this.TicketsCreated}}
            </p>
          </li>
          {{/each}}
        </ul>
        </br>
        </br>
      </div>
      <div class="col s12 m2">
      </div>
    </div>
  </div>
</div>